{
  "extends": "plugin:jasmine/recommended",
  "env": {
    "jasmine": true
  },
  "globals": {
    "sinon": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "plugins": ["jasmine"],
  "rules": {
    "jasmine/no-spec-dupes": [2, "branch"],
    "jasmine/no-suite-dupes": [2, "branch"]
  }
}
