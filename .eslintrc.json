{
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "class-methods-use-this": 1,
    "import/prefer-default-export": 0,
    "no-restricted-syntax": 0,
    "react/jsx-filename-extension": 0
  }
}
